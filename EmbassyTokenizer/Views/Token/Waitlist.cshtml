@model Dictionary<string, List<EmbassyTokenizer.Models.Token>>

<table class="table">
    <thead>
        <tr>
            <th>Med</th>
            <th>TR</th>
            <th>BS</th>
            <th>GO</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Values.Max(list => list.Count); i++)
        {
            <tr>
                @foreach (var categoryTokens in Model.Values)
                {
                    var token = categoryTokens.ElementAtOrDefault(i);
                    if (token != null)
                    {
                        <td style="text-align: center;">
                            @if (token.IsCalled)
                            {
                                <strong>@token.Category - @token.Number</strong>
                            }
                            else
                            {
                                <p>@token.Category - @token.Number</p>
                            }
                        </td>
                    }
                    else
                    {
                        <td></td>
                    }
                }
            </tr>
        }
    </tbody>
</table>
